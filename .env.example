# Cloudflare API (for DNS management + Let's Encrypt DNS-01 challenge)
# Get from: https://dash.cloudflare.com → Profile → API Tokens → "Edit zone DNS" template
CF_Token=
CF_Zone_ID=

# COTURN shared secret (for ephemeral credential generation)
# Generate with: openssl rand -hex 32
COTURN_AUTH_SECRET=

# Grafana admin password
GRAFANA_ADMIN_PASSWORD=

# SendGrid SMTP credentials for AlertManager email alerts
ALERTMANAGER_SMTP_PASSWORD=
